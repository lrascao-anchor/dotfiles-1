alias dir='exa -liaF -s name --color=auto'
alias g='git'
alias gm='git machete'
alias h='[ ! -z `git rev-parse --show-cdup` ] && cd `git rev-parse --show-cdup || pwd`'
alias ll='ls -lisaF --color=auto'
alias k='kubectl'

alias a='cd ~/anchorage'
alias t='cd ~/anchorage/source/go/service/trading'
alias b='cd ~/anchorage/source/go/service/buyingpower'
alias r='cd ~/anchorage/source/go/service/risk'
alias fe-client="cd ~/anchorage/source/js/client-dashboard/"
alias lib='cd ~/anchorage/source/go/lib'

alias gcloud-auth='gcloud auth application-default login; gcloud auth login; gcloud auth configure-docker'
alias gcloud-workstation-start='gcloud compute instances start "luis-rascao-workstation"'
alias gcloud-workstation-ssh='gcloud compute ssh --zone "europe-west2-a" "luis-rascao-workstation"  --tunnel-through-iap --project "anchorage-workstations" --ssh-flag="-X"'
